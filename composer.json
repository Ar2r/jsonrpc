{
  "name": "tochka-developers/jsonrpc",
  "description": "JsonRpc extension for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Babushkin",
      "email": "darkdarin@tochka.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "illuminate/support": "5.7.*|5.8.*|^6.0",
    "illuminate/http": "5.7.*|5.8.*|^6.0",
    "illuminate/container": "5.7.*|5.8.*|^6.0",
    "illuminate/pipeline": "5.7.*|5.8.*|^6.0",
    "phpdocumentor/reflection-docblock": "~4.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Tochka\\JsonRpc\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tochka\\JsonRpc\\JsonRpcServiceProvider"
      ]
    }
  }
}
